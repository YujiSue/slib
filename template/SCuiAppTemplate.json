{
	"app": 
	{
		"type": $APP_TYPE,
		"name": $APP_NAME,
		"version": $APP_VER,
		"creator": $CREATOR,
		"develop": $DEVELOP_DATE,
		"license": $APP_LICENSE,
	},
	"command": 
	{
		$COMMAND: 
		{
			"require": [$REQUIRED_OPTION_LIST]
		},
		$COMMAND_LIST
	},
	"option": 
	{
		$OPTION: 
		{
			"short": $OPT_SHORT,
			"type", $OPT_TYPE,
			"default", $OPT_VALUE,
			"caption": $OPT_CAPTION,
			"description": $OPT_DESCRIPTION
		},
		$OPTION_LIST
	}
}